<!-- stylesheets="static/css/BookingSheets.css" -->

<AnchorPane prefHeight="500.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.booking.BookCustomerController">
    <Button fx:id="customerPopup" layoutX="425.0" layoutY="262.0" mnemonicParsing="false" onAction="#addCustomerPopup" prefHeight="26.0" prefWidth="150.0" text="Add" />
    <Button fx:id="goBackButton" layoutX="20.0" layoutY="22.0" mnemonicParsing="false" onAction="#goBackToPreviousPage" prefHeight="26.0" prefWidth="63.0" text="&lt;  back" />
