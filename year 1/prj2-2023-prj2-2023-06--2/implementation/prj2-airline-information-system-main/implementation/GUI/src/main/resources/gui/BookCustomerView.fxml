        <font>
            <Font size="18.0" />
        </font>
    </Label>
    <Label layoutX="458.0" layoutY="22.0" text="Create Booking">
        <font>
            <Font name="System Italic" size="13.0" />
        </font>
    </Label>
</AnchorPane>
