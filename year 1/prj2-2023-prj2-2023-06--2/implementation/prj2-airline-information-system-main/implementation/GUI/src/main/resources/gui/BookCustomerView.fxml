            <Font size="18.0" />
        </font>
    </Label>
    <TableView fx:id="customerTable" layoutX="150.0" layoutY="162.0" prefHeight="76.0" prefWidth="700.0">
        <columns>
            <TableColumn fx:id="customerID" prefWidth="70.0" text="Id" />
